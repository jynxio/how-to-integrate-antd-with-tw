{
  "extends": ["stylelint-config-standard"],
  "rules": {
    "property-no-unknown": [
      true,
      {
        "ignoreProperties": ["composes"]
      }
    ],
    "selector-pseudo-class-no-unknown": [
      true,
      {
        "ignorePseudoClasses": ["global", "local"]
      }
    ],
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["theme", "utility"]
      }
    ],
    "custom-property-pattern": null,
    "import-notation": null
  }
}
